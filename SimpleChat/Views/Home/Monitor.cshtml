@{
    Layout = null;
}

<!DOCTYPE html>

<html ng-app="signalrMonitor">
<head>
    <title>Monitor</title>
    <script src="~/Scripts/libs/jquery-1.8.2.js"></script>

</head>
<body>
    <div ng-controller="mainController">
        <h2>Logs</h2>
        <ul>
            <li ng-repeat="log in logs">{{ log.eventType }} {{log.connectionId}}</li>
        </ul>
    </div>
    <script src="~/Scripts/libs/jquery.signalR-1.0.0.js"></script>
    <script src="/signalr/hubs"></script>
    <script src="~/Scripts/libs/angular.js"></script>
    <script src="~/Scripts/app/monitor.js"></script>
</body>
</html>
